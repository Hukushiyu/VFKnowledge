<script>
// Auto-save functionality using localStorage
const STORAGE_KEY = 'timberland_knowledge_extraction';

// Load saved data on page load
window.addEventListener('DOMContentLoaded', function() {
	loadSavedData();
	updateProgress();
	setupAutoSave();
	updateSaveTimestamp();
});

// Load saved data from localStorage
function loadSavedData() {
	const savedData = localStorage.getItem(STORAGE_KEY);
	if (savedData) {
		try {
			const data = JSON.parse(savedData);
			
			// Check if this is old format data (no questions array or questions don't start with "Question:")
			const isOldFormat = !data.questions || (data.questions && data.questions.length > 0 && !data.questions[0].startsWith('Question:'));
			
			if (isOldFormat) {
				console.log('‚ö†Ô∏è Old format detected. Please review your questions and update them to the new format.');
				showSaveStatus('Old data format detected - please review', 'success');
				// Don't load old question format, only load answers and checkboxes
			} else {
				// Restore question textareas
				document.querySelectorAll('textarea.fill-in-question').forEach((textarea, index) => {
					if (data.questions && data.questions[index]) {
						textarea.value = data.questions[index];
					}
				});
			}
			
			// Always restore answer textareas
			document.querySelectorAll('textarea.fill-in').forEach((textarea, index) => {
				if (data.textareas && data.textareas[index]) {
					textarea.value = data.textareas[index];
				}
			});
			
			// Restore checkboxes
			document.querySelectorAll('input[type="checkbox"]').forEach((checkbox, index) => {
				if (data.checkboxes && data.checkboxes[index] !== undefined) {
					checkbox.checked = data.checkboxes[index];
				}
			});
			
			console.log('‚úÖ Data loaded successfully');
		} catch (e) {
			console.error('Error loading saved data:', e);
		}
	}
}

// Save data to localStorage
function saveData() {
	const data = {
		questions: [],
		textareas: [],
		checkboxes: [],
		timestamp: new Date().toISOString()
	};
	
	// Save question textareas
	document.querySelectorAll('textarea.fill-in-question').forEach(textarea => {
		data.questions.push(textarea.value);
	});
	
	// Save answer textareas
	document.querySelectorAll('textarea.fill-in').forEach(textarea => {
		data.textareas.push(textarea.value);
	});
	
	// Save checkboxes
	document.querySelectorAll('input[type="checkbox"]').forEach(checkbox => {
		data.checkboxes.push(checkbox.checked);
	});
	
	localStorage.setItem(STORAGE_KEY, JSON.stringify(data));
	updateSaveTimestamp();
	updateProgress();
	showSaveStatus('Saved', 'success');
}

// Setup auto-save listeners
function setupAutoSave() {
	// Auto-save on question textarea change
	document.querySelectorAll('textarea.fill-in-question').forEach(textarea => {
		textarea.addEventListener('input', debounce(saveData, 1000));
		textarea.addEventListener('blur', saveData);
	});
	
	// Auto-save on answer textarea change
	document.querySelectorAll('textarea.fill-in').forEach(textarea => {
		textarea.addEventListener('input', debounce(saveData, 1000));
		textarea.addEventListener('blur', saveData);
	});
	
	// Auto-save on checkbox change
	document.querySelectorAll('input[type="checkbox"]').forEach(checkbox => {
		checkbox.addEventListener('change', saveData);
	});
}

// Debounce function to limit save frequency
function debounce(func, wait) {
	let timeout;
	return function executedFunction(...args) {
		const later = () => {
			clearTimeout(timeout);
			func(...args);
		};
		clearTimeout(timeout);
		timeout = setTimeout(later, wait);
	};
}

// Update save timestamp
function updateSaveTimestamp() {
	const savedData = localStorage.getItem(STORAGE_KEY);
	if (savedData) {
		try {
			const data = JSON.parse(savedData);
			if (data.timestamp) {
				const date = new Date(data.timestamp);
				const timeString = date.toLocaleTimeString();
				document.getElementById('saveStatus').textContent = `Last saved: ${timeString}`;
			}
		} catch (e) {
			console.error('Error reading timestamp:', e);
		}
	}
}

// Update progress statistics
function updateProgress() {
	const textareas = document.querySelectorAll('textarea.fill-in');
	const checkboxes = document.querySelectorAll('input[type="checkbox"]');
	
	let filledTextareas = 0;
	textareas.forEach(textarea => {
		if (textarea.value.trim().length > 0) {
			filledTextareas++;
		}
	});
	
	let checkedBoxes = 0;
	checkboxes.forEach(checkbox => {
		if (checkbox.checked) {
			checkedBoxes++;
		}
	});
	
	const totalItems = textareas.length;
	const percentage = totalItems > 0 ? Math.round((filledTextareas / totalItems) * 100) : 0;
	
	document.getElementById('progressStats').innerHTML = `
		Progress: ${percentage}% ‚Ä¢ ${filledTextareas}/${totalItems} fields ‚Ä¢ ${checkedBoxes}/${checkboxes.length} URLs
	`;
}

// Show save status message
function showSaveStatus(message, type) {
	const statusEl = document.getElementById('saveStatus');
	statusEl.textContent = message;
	
	// Brief highlight effect
	statusEl.style.transition = 'all 0.3s';
	statusEl.style.transform = 'scale(1.05)';
	setTimeout(() => {
		statusEl.style.transform = 'scale(1)';
	}, 300);
}

// Export data as JSON
function exportData() {
	const savedData = localStorage.getItem(STORAGE_KEY);
	if (!savedData) {
		alert('No data to export. Please fill out the form first.');
		return;
	}
	
	const data = JSON.parse(savedData);
	
	// Create a more readable export format
	const exportData = {
		exportDate: new Date().toISOString(),
		lastSaved: data.timestamp,
		topics: []
	};
	
	// Get all topics
	const topics = document.querySelectorAll('h2');
	let textareaIndex = 0;
	
	topics.forEach(topic => {
		if (topic.id !== 'timberland-knowledge-article-extraction') {
			const topicData = {
				title: topic.textContent,
				fields: []
			};
			
			// Get all questions in this topic
			let currentElement = topic.nextElementSibling;
			while (currentElement && currentElement.tagName !== 'H2' && currentElement.tagName !== 'HR') {
				if (currentElement.tagName === 'P' && currentElement.querySelector('strong')) {
					const question = currentElement.querySelector('strong').textContent;
					const textarea = currentElement.nextElementSibling;
					if (textarea && textarea.tagName === 'TEXTAREA') {
						topicData.fields.push({
							question: question,
							answer: data.textareas[textareaIndex] || ''
						});
						textareaIndex++;
					}
				}
				currentElement = currentElement.nextElementSibling;
			}
			
			if (topicData.fields.length > 0) {
				exportData.topics.push(topicData);
			}
		}
	});
	
	// Download as JSON file
	const blob = new Blob([JSON.stringify(exportData, null, 2)], { type: 'application/json' });
	const url = URL.createObjectURL(blob);
	const a = document.createElement('a');
	a.href = url;
	a.download = `timberland_knowledge_extraction_${new Date().toISOString().split('T')[0]}.json`;
	document.body.appendChild(a);
	a.click();
	document.body.removeChild(a);
	URL.revokeObjectURL(url);
	
	showSaveStatus('Exported successfully!', 'success');
}

// Clear all data
function clearAllData() {
	if (confirm('‚ö†Ô∏è Are you sure you want to clear ALL data? This cannot be undone!')) {
		if (confirm('Really sure? This will delete all your progress.')) {
			localStorage.removeItem(STORAGE_KEY);
			
			// Clear question textareas
			document.querySelectorAll('textarea.fill-in-question').forEach(textarea => {
				textarea.value = textarea.getAttribute('placeholder') || '';
			});
			
			// Clear answer textareas
			document.querySelectorAll('textarea.fill-in').forEach(textarea => {
				textarea.value = '';
			});
			
			// Clear checkboxes
			document.querySelectorAll('input[type="checkbox"]').forEach(checkbox => {
				checkbox.checked = false;
			});
			
			updateProgress();
			showSaveStatus('All data cleared', 'success');
		}
	}
}

// Add new Q&A pair
function addQAPair(button) {
	const questionsContainer = button.previousElementSibling;
	
	const qaPair = document.createElement('div');
	qaPair.className = 'qa-pair';
	
	const questionTextarea = document.createElement('textarea');
	questionTextarea.className = 'fill-in-question';
	questionTextarea.placeholder = 'Enter your question here...';
	questionTextarea.value = 'Question: for example, "Your question here"';
	
	const deleteBtn = document.createElement('button');
	deleteBtn.className = 'btn-delete';
	deleteBtn.onclick = function() { deleteQAPair(this); };
	deleteBtn.title = 'Delete this Q&A pair';
	deleteBtn.textContent = '√ó';
	
	const br = document.createElement('br');
	
	const answerTextarea = document.createElement('textarea');
	answerTextarea.className = 'fill-in';
	answerTextarea.placeholder = 'Fill in your answer here...';
	
	qaPair.appendChild(questionTextarea);
	qaPair.appendChild(deleteBtn);
	qaPair.appendChild(br);
	qaPair.appendChild(answerTextarea);
	
	questionsContainer.appendChild(qaPair);
	
	// Setup auto-save for new elements
	questionTextarea.addEventListener('input', debounce(saveData, 1000));
	questionTextarea.addEventListener('blur', saveData);
	answerTextarea.addEventListener('input', debounce(saveData, 1000));
	answerTextarea.addEventListener('blur', saveData);
	
	saveData();
	showSaveStatus('Q&A pair added', 'success');
}

// Delete Q&A pair
function deleteQAPair(button) {
	if (confirm('Are you sure you want to delete this Q&A pair?')) {
		const qaPair = button.closest('.qa-pair');
		qaPair.remove();
		saveData();
		showSaveStatus('Q&A pair deleted', 'success');
		updateProgress();
	}
}
</script>
